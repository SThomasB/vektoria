

factorialTail = (n acc,
               ? n == 0 -> acc
               | (factorialTail (n-1) (n*acc))
)


--let in expressions -> ((n a, @print (factorial n 1)) 10 20)

start=<-@cpuTime
<-let n=40 in (@print
    "----------------Factorial---------------"
    ""
    "Factorial "+(n)+" is "+(factorialTail (n) 1)
    ""
    ""
    "----------------endprogram--------------"
)
end=<-(@cpuTime)
<-@print (end-start)/1000000
