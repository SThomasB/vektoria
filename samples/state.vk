
::pair a b = (q, (q a b))
::fst a b = a
::snd a b = b
::s f = (q, (q f))
::un f = f 
::app sa x = ((un sa) x)
::fmap t sa = (s 
                (x, let ::state = ((un sa) x) 
                    in let :: a = (state fst)
                           :: b = (state snd)
                    in (pair (t a) b)
                ))
